    <div id="main-wrapper">


        <partial name="NavHeaderPartial.cshtml" />

        <partial name="HeaderPartial.cshtml" />

        <partial name="SidebarPartial.cshtml" />

        <div class="content-body">

            <div class="row page-titles mx-0">
